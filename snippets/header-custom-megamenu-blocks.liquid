<div class="nav-menu-dropdown page-width">
  <div class="menu-dropdown-content">
    <div class="main-menu-block">
      <ul class="first-level-menu first-level-desktop">
        {% for block in section.blocks %}
          {% if block.type == 'main-megamenu' %}
            <li>
              <a href="#">{{ block.settings.navigation_title }}</a>
            </li>
          {% endif %}
        {% endfor %}
      </ul>
      {% for block in section.blocks %}
        {% if block.type == 'shop-megamenu' or block.type == 'inner-megamenu' %}
          <div class="first-level-menu_items">
            <div class="first-level-shop_list">
              <h3>{{ block.settings.color_menu_title }}</h3>
              {% if block.settings.secondary_menu != blank %}
                <ul class="second-level-menu-list">
                  {% for link in linklists[block.settings.secondary_menu].links %}
                    <li>
                      <a href="{{ link.url }}">{{ link.title }}</a>
                    </li>
                  {% endfor %}
                </ul>
              {% endif %}
            </div>
            {% if block.settings.image_block_image != blank %}
              <div class="first-level-shop_image-block">
                <span class="image_level">
                  <img
                    src="{{ block.settings.image_block_image | image_url }}"
                    width="282"
                    height="266"
                    alt=""
                    loading="lazy"
                  >
                </span>
              </div>
            {% endif %}
            {% if block.settings.featured_image != blank %}
              <div class="first-level-card">
                <div class="main-card_item">
                  <span class="image-span">
                    <img
                      src="{{ block.settings.featured_image | image_url }}"
                      width="432"
                      height="266"
                      alt=""
                      loading="lazy"
                    >
                  </span>
                  <div class="main-card_item_text">
                    <span>{{ block.settings.featured_top_text }}</span>
                    <h2>{{ block.settings.featured_title }}</h2>
                    {% if block.settings.featured_cta_link != blank %}
                      <a href="{{ block.settings.featured_cta_link }}">
                        {{ block.settings.featured_cta_text }}
                      </a>
                    {% endif %}
                  </div>
                </div>
              </div>
            {% endif %}
          </div>
        {% endif %}
      {% endfor %}
      <!-- Shop by Brand Logos -->
      <div class="first-level-menu_items logo-item">
        <div class="first-level-shop_list">
          <h3>Shop By Brand</h3>
          <ul class="second-level-menu-list">
            {% for i in (1..8) %}
              {% assign logo = section.settings.logo_image %}
              {% if logo != blank %}
                <li>
                  <a href="#">
                    <img src="{{ logo | image_url }}" width="100" height="38" alt="" loading="lazy">
                  </a>
                </li>
              {% endif %}
            {% endfor %}
          </ul>
        </div>
        <!-- Static Products (can be made dynamic later using product handles or blocks) -->
        <div class="row">
          {% assign images = section.settings | json | parse_json %}
          {% assign product_images = images.product_image | split: ',' %}
          {% for i in (1..3) %}
            <div class="product-main_box">
              <div class="product-block">
                <img
                  src="{{ section.settings['product_image'] | append: [i] | image_url }}"
                  width="206"
                  height="206"
                  loading="lazy"
                >
              </div>
              <div class="product_content">
                <a href="#"><h3>Suzuki Wedged Baseball T-Shirt</h3></a>
                <div class="add_to_card">
                  {%- render 'buy-buttons',
                    block: block,
                    product: product,
                    product_form_id: product_form_id,
                    section_id: section.id
                  -%}
                </div>
              </div>
              <a href="#"></a>
            </div>
          {% endfor %}
        </div>
        <!-- Repeat .row as needed -->
      </div>
    </div>
  </div>
</div>
